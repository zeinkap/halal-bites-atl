{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "halal-restaurants-atl",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "checkLintErrors": "npx eslint src --ext .ts,.tsx",
    "checkTSErrors": "tsc --noEmit",
    "seed": "ts-node prisma/seed.ts",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "clear-redis-cache": "ts-node scripts/clear-redis-cache.ts",
    "migrate-prod": "dotenv -e .env.production -- npx prisma migrate deploy",
    "backup-prod-db": "dotenv -e .env.production -- pg_dump \"$DATABASE_URL\" > prod-backup-$(date +%Y%m%d-%H%M%S).sql",
    "seed-prod": "dotenv -e .env.production -- npx ts-node --compiler-options '{\"module\":\"CommonJS\"}' prisma/seed.ts",
    "clear-redis-cache-prod": "dotenv -e .env.production -- npx ts-node scripts/clear-redis-cache.ts"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.787.0",
    "@headlessui/react": "^2.2.2",
    "@heroicons/react": "^2.2.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "@stripe/stripe-js": "^7.3.0",
    "@types/cookie": "^0.6.0",
    "@types/nodemailer": "^6.4.17",
    "@upstash/redis": "^1.34.8",
    "bcryptjs": "^3.0.2",
    "chalk": "^5.4.1",
    "chart.js": "^4.4.9",
    "cloudinary": "^2.6.0",
    "clsx": "^2.1.0",
    "cookie": "^1.0.2",
    "dotenv": "^16.5.0",
    "intersection-observer": "^0.12.2",
    "next": "^15.3.1",
    "next-auth": "^4.24.11",
    "nodemailer": "^6.10.1",
    "react": "^19.0.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.56.1",
    "react-hot-toast": "^2.5.2",
    "react-stars": "^2.2.5",
    "react-textarea-autosize": "^8.5.3",
    "sharp": "^0.33.2",
    "stripe": "^18.1.1",
    "tailwind-merge": "^2.2.1",
    "typescript": "^5.3.3",
    "uuid": "^11.1.0",
    "yet-another-react-lightbox": "^3.17.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.51.1",
    "@prisma/client": "^6.7.0",
    "@react-google-maps/api": "^2.20.6",
    "@types/estree": "^1.0.7",
    "@types/google.maps": "^3.58.1",
    "@types/node": "^20.17.30",
    "@types/react": "19.1.2",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.19",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.2.3",
    "jiti": "^2.4.2",
    "postcss": "^8.4.38",
    "prisma": "^6.9.0",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3"
  }
}
